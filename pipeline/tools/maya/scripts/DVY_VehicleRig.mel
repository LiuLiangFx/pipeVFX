					- DVY_VehicleRig -
						  
	This script will build Vehicle rig for full vehicle rigging, suspension
	and steering. 

	
	I am currently working on building a full suite ot scripts to allow rigging
	of all the various parts of any wheeled vehicle. The intention is to build a
	set of "modular" scripts to allow rigging of individual parts and or a fullly
	rigged vehicle.

*/
	

//============================================================================
//	GLOBAL VARIABLES
//============================================================================
	global int $DVYDebug;

// Integer Bollean Flag that allows print statements to be output to
// message field for tracking purposes.


//============================================================================



// Set and Clear Global strings of current Model (unrigged) used for setting up various sections of the Rig etc.

	global string $hier[];	global string $JThier[];	global string $LChier[];

global proc DVY_VehicleRig()
 {
		//	Start of DVY_VehicleRig Procedure
		
		global int $DVYDebug;

	//	Check for any "Window History" and delete if it exists.
	 	if (`windowPref -exists DVY_VehicleUI`)
			windowPref -edit
			-topLeftCorner 150 140 -widthHeight 1050 750 -parentMain true DVY_VehicleUI;
	
	//	Check if Windows exists and delete is true.
		if (`window -q -ex DVY_VehicleUI`) deleteUI DVY_VehicleUI;	

	//	Name of UI for DVY_VehicleRig Setup.	
		window -menuBar true 
			-sizeable 1
			-w 1050 -h 750 
			-title "Davvey's Vehicle Rig Setup Beta ver 1.0 (Draft Version for feedback)" 
			DVY_VehicleUI;

	menuBarLayout;

		// Create a tearOff menu
			menu -label "File" -tearOff true;
				// add the menu items
				menuItem -label "New";
				menuItem -label "Open";
				menuItem -label "Save";
				
			menuItem -divider true;
				menuItem -label "Quit";
			
		// add a help menu
			menu -label "Help" -helpMenu true;
				menuItem -label "About Application...";
	
//--------------------------------------------------------------------------------------
//	
//							End of MenuBar Panel Layout
//
//--------------------------------------------------------------------------------------
				
	string $PaneLayout = `paneLayout -height 600 -paneSize 1 22 55 -configuration "left3" "DVY_Rig_PaneLayout"`;
			
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//	
//				Start Of Outliner Panel Layout to allow selection of Vehicle Parts
//
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++	
	
	// THIS IS THE OUTLINER PANEL
		string $OutlinerFrame = `frameLayout -borderStyle "etchedOut" -collapsable false -label "DVY_Outliner PanelInfo" -labelVisible false` ;
	
		string $panel = `outlinerPanel -menuBarVisible false`;
			string $outliner = `outlinerPanel -query -outlinerEditor $panel`;
			outlinerEditor -edit
				-mainListConnection "worldList" 
				-selectionConnection "modelList" 
				-showShapes false
				-showAttributes false
				-showConnected false
				-showAnimCurvesOnly false
				-autoExpand false
				-showDagOnly true
				-ignoreDagHierarchy false
				-expandConnections false
				-showCompounds false
				-showNumericAttrsOnly false
				-highlightActive true
				-autoSelectNewObjects false
				-doNotSelectNewObjects false
				-transmitFilters false
				-showSetMembers false
				-setFilter DefaultGeometryFilter
			$outliner;
			
		setParent..;
	setParent..;
	
//--------------------------------------------------------------------------------------
//	
//							End of Outliner Panel Layout
//
//--------------------------------------------------------------------------------------

	//	Define a Pane Layout for all the  rigging tabs and the selected Items Table.
		string $RigPaneLayout = `paneLayout -paneSize 1 66 75 -paneSize 2 34 100 -configuration "left3" "RigPane"`;	
	
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//	
//						Start of The Vehicle Rigging Panel Layout
//
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	
	// Create a Frame Layout for all the Rigging Pane Tabs etc.
		string $RigPaneFrame = `frameLayout -borderStyle "etchedOut" -collapsable false -label "RigPaneLayout" -labelVisible false` ;

	// Create a Column Layout for all the Rigging Pane Tabs etc.
		string $RigPaneColumn = `columnLayout -adjustableColumn true 
									-columnWidth 550 -columnAttach "both" 1 "RigPaneColumn"`;	

	// Define the Row Layout for the Selected Table Iems Layout Section.
		string $RigPaneRow1 = `rowLayout -numberOfColumns 1 -rowAttach 1 "top" 1
				-adjustableColumn 1 -columnAlign 1 "center" -columnAttach 1 "both" 1 "RigPaneRow1"`;			
		
	setParent..;	
		
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//	
//						Start Of Tab Layout to hold Vehicle Tab Layouts
//
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++	
		
	string $Tabs = `tabLayout -innerMarginWidth 1 -innerMarginHeight 1 	 "TabLayout"`;
		
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//	
//							Start of Setup and Info Tab
//
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++			
	
	string $Tab01 = `columnLayout -adjustableColumn true -columnAttach "both" 1 "Setup"`;	
		
	// Create a Frame Layout to hold Setup Info etc.
		string $SetupInfoFrame = `frameLayout -borderStyle "etchedOut" -collapsable false -label "Setup Info" -labelVisible false` ;
				
	// Create a Layout for Title Section of the Setup Frame.
		string $SetupInfoColumn = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupInfoColumn"`;	
	
	//Title and Info Section for the "Initial Setup" tab. 
		text -height 5 -label "" ;	//Spacer
			separator -height 5 -style "double" ;
			text -bgc .5 .5 1 -height 20 -align "center" -font "boldLabelFont" -label "Welcome to Davvey's Vehicle Rigging Setup.";
			separator -height 5 -style "double";
			text -height 5 -label "" ;	//Spacer
			
//***********************************************************************************	
				
	// Create a collapsible frame layout
		string $SetupInfoFrameC = `	frameLayout -collapsable true -collapse false -label "  Info"` ;	
		
	// Create a collapsable Frame for Setup Info.
		string $SetupInfoColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupInfoColumnC"`;	

		separator -height 5 ;	// Spacer 		
			text -bgc 1 1 .85 -height 10 -label "" ;	//Space
			text -bgc 1 1 .85 -align "left" -font "boldLabelFont" -label " This a draft copy of the GUI I am building for Vehicle Rigging any comments good or\n bad please let me know. The UI will not run as there are none of the accompanying\n scripts included.\n\n Please contact me at Davvey@ntlworld.com with feedback thanks.\n\n Each Field is disabled until the prevous field has been 'Intialized'.\n I have tried to make the 'Rigging Setup' flow in a logical form.";
			text -bgc 1 1 .85 -height 10 -label "" ;	//Spacer
			separator -height 5 -style "double";
			text -height 5 -label "" ;	//Spacer
		
	setParent $SetupInfoColumn ;
	
	// End of Title Set up Info Inroduction.
			
//***********************************************************************************	
	
	// Create a Collapsable a Frame to hold Vehicle Name Section.
		string $SetupVehNameFrame = `frameLayout -borderStyle "etchedOut" -collapsable true -collapse true -label "  Vehicle Name Section"` ;
		
	// Define the layout of controls added to the Vehicle Name Section.
		string $SetupVehNameColumn = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupVehNameColumn"`;		
		
		separator -height 5 ;	// Spacer
			text -bgc 1 1 .85 -height 5 -label "" ;	//Space	
			text -bgc 1 1 .85 -align "left" -label "  Please enter name to be used for the Vehicle during the setup process and for the completed rigged vehicle.\n  Note :- Please ensure the name contains only Letters, Numbers and or \"Underscore\" i.e. no spaces etc.";
			text -bgc 1 1 .85 -height 5 -label "" ;	//Spacer
			separator -height 5 -style "double";
			text -height 10 -label "" ;	//Spacer

		setParent..;
	setParent..;
			
//***********************************************************************************	
				
	// Create a Non collapsible frame layout for Vehicle Name Section
		string $SetupVehNameFrameC =`frameLayout -collapsable false -label "  Vehicle Name Section" -labelVisible false` ;
	
	// Define the layout of controls added to the Vehicle Name Section.
		string $SetupVehNameColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupVehNameColumnC"`;		

		text  -height 5 -label "" ;	//Space	
		
	//	RowLayout for Entering the Vehicle's new Name.
		rowLayout -nc 2
			-columnWidth 1 300 -columnAttach 1 "both" 2 -columnAlign 1 "center" -adjustableColumn 1
			-columnWidth 2 110 -columnAttach 2 "both" 2 -columnAlign 2 "center" ;

		textField -text "" -ann "Type New Name for Car after Rigging"
			-editable true -enable true VehicleName;

		button -bgc .5 1 .5 -label "Vehicle Name" -enable true
			-command "select -clear;\DVY_AddTxtToList(\"VehicleName\", \"SelectedList\",1,4);"

				VehicleNameButton;	
		
	setParent $SetupVehNameColumnC ;
			
//***********************************************************************************	
	
		text -height 5 -label "" ;	//Spacer
		
		setParent..;
	setParent..;
			
//***********************************************************************************	
		
	// Create a collapsible frame layout for Vehicle type Radio Button Collection.
		string $SetupVehTypeFrame = `frameLayout -borderStyle "etchedOut" -collapsable true -collapse true -label "  Vehicle Type Section"` ;
		
	// Define the layout for the controls for the Vehicle type Radio Button Collection.
		string $SetupVehTypeColumn = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupVehTypeColumn"`;		

		
	// Title Section for Vehicle Type Selection.
		separator -height 5 ;
		text -bgc 1 1 .85 -height 5 -label "" ;	//Space
		text -bgc 1 1 .85 -align "left" -label "    Please Select the Vehicle Type. If you are Unsure of the type or style of the Vehicle please to see Help Section for more Info.";
		text -bgc 1 1 .85 -height 5 -label "" ;	//Spacer
		separator -height 5 -style "double";
		text -height 5 -label "" ;	//Spacer
		
		setParent.. ;
	setParent.. ;	
			
//***********************************************************************************	
		
	setParent $Tab01 ;
			
//***********************************************************************************	
		
	//	Vehicle Type Radio button selection Group
	//	This will set various selection options on each tab.
		
	// Create a Non collapsible frame layout for Vehicle type Radio Button Collection.
		string $SetupVehTypeFrameC = `frameLayout -collapsable false -label "  Vehicle Type Section" -labelVisible false` ;

	// Define the layout for the controls for the Vehicle type Radio Button Collection.
		string $SetupVehTypeColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupVehTypeColumnC"`;		
	
		text -height 5 -label "" ;	//Spacer			
		
	// Radio Collection Group
		string $VehicleTypeRadioCol = `radioButtonGrp -numberOfRadioButtons 4
		 		-label "Vehicle Type :-" -columnAlign 1 "right" 
				-columnWidth5 90 100 90 80 110
				-labelArray4 "Car" "Truck only"  "Trailer" "Truck + Trailer"`;

			text -height 2 -label "" ;	//Spacer

			radioButtonGrp -numberOfRadioButtons 4 -shareCollection $VehicleTypeRadioCol
			 		-label "" -columnAlign 1 "left" 
					-columnWidth5 90 100 90 80 110
					-labelArray4  "Steerable Trailer" "Bike" "Trike" "Unicycle" ;
		
				text -height 2 -label "" ;	//Spacer	

			radioButtonGrp -numberOfRadioButtons 4 -shareCollection $VehicleTypeRadioCol
			 		-label "" -columnAlign 1 "left" 
					-columnWidth5 90 100 90 80 110
					-labelArray4 "Half Tracked"  "Full Tracked" "Other"	"????"  
			"VehicleTypeRadioButtons";
			
			
		
	// End of Radio Collection Group	
		text  -height 2 -label "";		// Spacer		
			
	// End of Vehicle Type Selection Frame
			
	setParent $Tab01 ;
			
//***********************************************************************************	
	
	// Create a Collapsible frame layout for Selecting The Vehicles Current "Top Node".
		string $SetupVehTopNodeFrameC = `frameLayout -borderStyle "etchedOut" -collapsable true -collapse true -label "  Vehicle Top Node Section"` ;

	// Define the layout for the Collapsable section of the Vehicle Top Node Section.
		string $SetupVehTopNodeColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupVehTopNodeColumnC"`;		

	// Title Section for Vehicle's current Top Node
		separator -height 5 ;
		text -bgc 1 1 .85 -height 5 -label "" ;	//Space
		text -bgc 1 1 .85 -align "left" -label "    Please select the Vehicle's current top node and press OK.";
		text -bgc 1 1 .85 -height 5 -label "" ;	//Spacer
		separator -height 5 -style "double";
		text -height 10 -label "" ;	//Spacer	
		
		setParent..;
	setParent..;
	
//***********************************************************************************	
		
	// Create a Non Collapsible frame layout for Selecting The Vehicles Current "Top Node".
		string $SetupVehTopNodeFrame = `frameLayout -collapsable false -labelVisible false -label "  Vehicle Top Node Section"` ;
		
	// Define the layout for Non Callpsible section of the Vehicle Top Node Section.
		string $SetupVehTopNodeColumn = `columnLayout -adjustableColumn true -columnAttach "both" 1 "SetupVehTopNodeColumn"`;		
	
		text  -height 5 -label "" ;	//Spacer	
			
//***********************************************************************************	
			
	//	RowLayout for Entering the Vehicle's Top Node.
		rowLayout -nc 2
			-columnWidth 1 300 -columnAttach 1 "both" 2 -columnAlign 1 "center" -adjustableColumn 1
			-columnWidth 2 110 -columnAttach 2 "both" 2 -columnAlign 2 "center" ;

		textField -text "" -editable true -enable true VehicleTopNode;

		button -bgc .5 1 .5 -label "Vehicle Top Node" -enable true
				-command "\DVY_AddToLists(\"VehicleTopNode\", \"SelectedList\",2,4);"
			VehicleTopNodeButton;


		setParent $SetupVehNameColumnC ;
	
	// End of Vehicle Top Node selection section
		
			
//***********************************************************************************	
		
		text -height 5 -label "" ;	//Spacer
	
	setParent $Tab01 ;
			
//***********************************************************************************	
	
	//End of Vehicle Setup Tab
			
	setParent $Tabs ;
	
//-------------------------------------------------------------------------------------
//	
//							End of Setup and Info Tab
//
//-------------------------------------------------------------------------------------

	
	
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//	
//							Start Of Wheels and Steering Tab Layout
//
//+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	
	// Start Of Initial Wheels Tab Layout..
	
		global string $WheelsInfoFrameC ;		global string $WheelsInfoFrame ;		
		global string $WheelsSteerFrameC ;		global string $WheelsSteerFrame ;
		global string $WheelsFrontFrameC ;		global string $WheelsFrontFrame ;
		global string $WheelsRearFrameC ;		global string $WheelsRearFrame ;
		
		
	string $Tab02 = `columnLayout -adjustableColumn true -columnAttach "both" 1 "Wheels"`;		
	
	string $WheelsRowLayout = `rowLayout -numberOfColumns 2
								-columnWidth 1 150 -columnAlign 1 "left" -columnAttach 1 "both" 1
								-columnWidth 2 350 -columnAlign 2 "left" -columnAttach 2 "both" 1
							"WheelsRow"`;	
			
//***********************************************************************************	
		
	// Wheels CheckBox Section to enable the Wheels Tab.
		checkBox -label "Enable Wheels Section" -align "left" 
			-onCommand 
						"checkBoxGrp -edit -enable true WheelsOptionsCheck;\
						frameLayout -edit -collapse true $WheelsInfoFrameC;\
						frameLayout -edit -collapse true $WheelsSteerFrameC;frameLayout -edit -enable true -visible true -collapse false $WheelsSteerFrame;\
						frameLayout -edit -collapse true $WheelsFrontFrameC;\
						frameLayout -edit -collapse true $WheelsRearFrameC;"
				
			-offCommand	
						"checkBoxGrp -edit -enable false WheelsOptionsCheck;\
						frameLayout -edit -collapse false $WheelsInfoFrameC;\
						frameLayout -edit -collapse false $WheelsSteerFrameC;frameLayout -edit -enable false -visible false -collapse false $WheelsSteerFrame;\
						frameLayout -edit -collapse false $WheelsFrontFrameC;\
						frameLayout -edit -collapse false $WheelsRearFrameC;"
			WheelsEnableCheck;						  

	// End of Wheels "Checkbox" Section. If not selected then no Door Options will be setup.
		
//		string $WheelsFrontandRearLayout = `columnLayout -adjustableColumn true -columnAttach "both" 1 "WheelsFrontandRear"`;
			
//***********************************************************************************	
			
	// Wheels Options "CheckBox Section Section to enable front and rear Wheels and the type of opening required.	
		checkBoxGrp -numberOfCheckBoxes 2 -labelArray2 "Front Wheels" "Rear Wheels" 
					-label "Wheels Options Buttons" 
					-enable1 false -enable2 false 
					-columnWidth 1 130 -columnAlign 1 "right" -columnWidth 2 110 -columnWidth 3 110 
					
				-onCommand1 "frameLayout -edit -enable true -visible true -collapse false $WheelsFrontFrame;"
				-onCommand2 "frameLayout -edit -enable true -visible true -collapse false $WheelsRearFrame;"
				
				-offCommand1 "frameLayout -edit -enable false -visible false -collapse false $WheelsFrontFrame;"
				-offCommand2 "frameLayout -edit -enable false -visible false -collapse false $WheelsRearFrame;"
			WheelsOptionsCheck;
	
	setParent..;	
	setParent $Tab02 ;
		
		// End of Wheels Options "Checkbox" Section. If not selected then no Wheels Options will be setup.
			
//***********************************************************************************	
		
	// Wheels Title Section 
		text -height 5 -label "" ;	//Spacer
		separator -height 5 -style "double" ;
		text -bgc .8 .8 1 -height 20 -align "center" -font "boldLabelFont" -label "Tab for setting up Wheels layout etc.";
		separator -height 5 -style "double";
		text -height 5 -label "" ;	//Spacer
			
//***********************************************************************************	
				
	// Create a collapsible frame layout for Wheels Info Section.
		string $WheelsInfoFrameC = `frameLayout -visible 1 -enable true -collapsable true -collapse true -label "  Wheels Info"` ;
				
	// Define the layout of controls added to the Wheels Info Frame.
		string $WheelsInfoColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "WheelsInfoColumnC"`;	

	//	Title and Info Section for the "Wheels Title Section". 
		separator -height 5 ;
		text -bgc 1 1 .85 -height 10 -label "" ;	//Space
		text -bgc 1 1 .85 -align "left" -label "  Please select the type of steering required for the Vehicle (See Help section for more info on the Vehicle Sections.\n  Use the Slider or Data entry field to set the no. of axles required for both Front and Rear of the Vehicle.\n  Select no of Wheels per Side of each Axle using Radio Buttons.\n  Then select each part of the relevant Wheel Group and press the corresponding \"OK  >>\" Button.";
		text -bgc 1 1 .85 -height 10 -label "" ;	//Spacer
		separator -height 5 -style "double";
		text -height 10 -label "" ;	//Spacer
		
	setParent $Tab02;
	
	// End of Wheels Title and Info Inroduction.
			
//***********************************************************************************	
		
	// 	Create a collapsible frame layout for Vehicle Steering Section.
		string $WheelsSteerFrameC = `frameLayout -borderStyle "etchedOut" -collapsable true -enable true -collapse true -label "  Vehicle Steering Section"` ;
		
	// 	Define the layout of controls added to the Vehicle Name Section.
		string $WheelsSteerColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "WheelsSteerColumnC"`;		
		

	// Define the layout of Steering Radio Button Group.
		separator -height 5 ;	// Spacer 
		text -bgc 1 1 .85 -height 5 -label "" ;	//Space	
		text -bgc 1 1 .85 -align "left" -label "  Please Select which type of steering option required.";
		text -bgc 1 1 .85 -height 5 -label "" ;	//Spacer
		separator -height 5 -style "double";
		text -height 5 -label "" ;	//Spacer
		
		setParent..;
	setParent $Tab02 ;
		
//***********************************************************************************	
			
	// Create a Non collapsible frame layout for Vehicle Steering Section.
		string $WheelsSteerFrame = `frameLayout -collapsable false -labelVisible false -label "  Vehicle Steering Section"` ;
		
	// Define the layout of controls added to the Vehicle Name Section.
		string $WheelsSteerColumn = `columnLayout -adjustableColumn true -columnAttach "both" 1 "WheelsSteerColumn"`;
	
		text -height 5 -label "" ;	//Spacer	
		
	// Steering Radio Button Group	
		
		radioButtonGrp -editable true -enable false
					-columnAlign 1 "right" 
					-columnWidth5 100 80 80 110 140
					-numberOfRadioButtons 4 
					-label "Steering Option:- " 
					-labelArray4 "Front" "Rear" "Both Combined" "Front + Rear Independant"
					-select 1
				SteeringRadioButtons ;
			
	//End of Steering Radio Button Group Section.
		text -height 5 -label "" ;	//Spacer
	
		setParent $WheelsSteerColumn;
	setParent $Tab02 ;
				
//***********************************************************************************	
	
	// Create a collapsible frame layout
		string $WheelsFrontFrameC = `frameLayout -borderStyle "etchedOut" -label "  Front Wheels Section"
									-visible true -labelVisible true -enable true
									-collapsable true -collapse false`;
	
	// Define the layout of controls added to the window.
		string $WheelsFrontColumnC = `columnLayout -adjustableColumn true -columnAttach "both" 1 "WheelsFrontColumnC"`;		
		
	// Spacer and Title Section for Front Wheels.
		separator -height 5 ;
		text -bgc 1 1 .85 -height 5 -label "" ;	//Space
		text -bgc 1 1 .85 -align "left" -label "  Front Wheels:- Select No. of Axles using Data Field or Slider, then select each item as required." ;
		text -bgc 1 1 .85 -height 5 -label "" ;	//Spacer
		separator -height 5 -style "double";
		text -height 5 -label "" ;	//Spacer
	
		setParent..;
	setParent..;
			
//***********************************************************************************	
		
	// Create a Non Collapsible frame for the Front Wheels Section.
		string $WheelsFrontFrame = `frameLayout -labelVisible false -label "  Front Wheels Section"
										 		-visible false -enable false -collapsable false` ;
	// Define the layout of controls added to the window.
		string $WheelsFrontColumn = `columnLayout -adjustableColumn true -columnAlign "center" -columnAttach "both" 1 "WheelsFrontColumn"`;		
	
		text -height 5 -label "" ;	//Spacer
		
	//	Front Axle Slider to set number of axles
		rowLayout -numberOfColumns 2
				  	-columnWidth 1 400 -columnAttach 1 "both" 1 -adjustableColumn 1
					-columnWidth 2 125 -columnAlign 2 "center" 
			frontAxleSliderSection ;
		
			int $FrontAxleSliderV = 0;
				
			intSliderGrp -columnAlign 1 "right" -columnAlign 2 "center" -columnAlign 3 "right"
					-adjustableColumn 3
					-columnWidth3 110 50 150
					-label "No. of Front Axles :-" 
					-field true
					-minValue 0 
					-maxValue 10
					-step 1
					-value 1
					-dragCommand "FrontAxleSliderCommand;"
					-changeCommand "FrontAxleSliderCommand;"
				FrontAxleSlider;
		
		button -bgc .5 1 .5 -label "Set No. of Front Axles" 
				-width 120 -command "$FrontAxleSliderV == `intSliderGrp -query -value FrontAxleSlider`;\print $FrontAxleSliderV;"
			FrontAxleButton;
		
		setParent..;
		
	//	End of Front Axle Slider to set number of axles		
			
//***********************************************************************************	
			
		separator -height 10 ;	// 	Spacer
		
	//	Column Setup for Left and Right Hand sides of the Vehicle
		rowLayout 
				-numberOfColumns 2
				-columnWidth2 275 275
				-columnAttach 1 "both" 1 -columnAlign 1 "center"
				-columnAttach 2 "both" 1 -columnAlign 2 "center" 
			WheelsLayout ;
		
	//	Left Hand Side of the Front Wheels Section
		columnLayout -adjustableColumn true
					-columnAttach "both" 1
					-columnAlign "center"
				LeftSideLayout;
		
			text -label "Left Side Of Vehicle";

	setParent..;
	
//***********************************************************************************	
	
	//	Right Hand Side of the Front Wheels Section		
		columnLayout -adjustableColumn true
					-columnAttach "both" 1
					-columnAlign "center"
				RightSideLayout;
		
			text -label "Right Side Of Vehicle";
			
	setParent LeftSideLayout;

//**********************************************************************************
	
	//	Start of Left Hand Side Front Wheels Button and Text Sections.
		rowLayout -nc 2  
				-columnWidth 1 170 -columnAttach 1 "both" 1 -columnAlign 1 "center" -adjustableColumn 1
				-columnWidth 2 105 -columnAttach 2 "both" 1 -columnAlign 2 "center"
			LeftHandWheelsLayout;	

	//	Left Hand Side Front Wheels Text Section.
		columnLayout -adjustableColumn true
					-columnWidth 170
					-columnAttach  "both" 1
					-columnAlign  "center"
				LeftFrontWheelText ;
	
			textField -text "" -editable true -enable true LfFrWheelGrp;
			textField -text "" -editable true -enable true LfFrWheel;
			textField -text "" -editable true -enable true LfFrTyre;

		setParent..;

//***********************************************************************************
		
	//	Left Hand Side Front Wheels Button Section.		
		columnLayout
				-columnWidth 105
				-columnAttach  "both" 1
				-columnAlign  "center"
			LeftFrontWheelButtons ;		

		button -bgc .5 1 .5 -label "Front Wheel Grp" -enable true
			-command "DVY_AddToLists(\"LfFrWheelGrp\", \"SelectedList\",6,4);"
		LfFrWheelGrpButton;
			
		button -bgc .5 1 .5 -label "Front Wheel" -enable true
			-command "DVY_AddToLists(\"LfFrWheel\", \"SelectedList\",7,4);"
			LfFrWheelButton;
		
		button -bgc .5 1 .5 -label "Front Tyre" -enable true
			-command "DVY_AddToLists(\"LfFrTyre\", \"SelectedList\",8,4);"
			LfFrTyreButton;		
			
	setParent RightSideLayout ;	
	
	//	End of Left Hand Side Front Wheels Button and Text Sections.

	
//***********************************************************************************
	
	
	//	Start of Right Hand Side Front Wheels Button and Text Sections.
		rowLayout -nc 2  
				-columnWidth 1 170 -columnAttach 1 "both" 1 -columnAlign 1 "center" -adjustableColumn 1
				-columnWidth 2 105 -columnAttach 2 "both" 1 -columnAlign 2 "center"
			RightHandWheelsLayout;	

	//	Right Hand Side Front Wheels Text Section.
		columnLayout -adjustableColumn true
					-columnWidth 170
					-columnAttach  "both" 1
					-columnAlign  "center"
